{
  "name": "cpprint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www",
    "start": "export DEBUG='cpprint:server' && node ./bin/www",
    "debug": "export NODE_ENV= && export DEBUG='cpprint:server,app:*,mdb:*' && nodemon ./bin/www",
    "vsdebug": "export NODE_ENV= && export DEBUG='cpprint:server,app:*' && nodemon --inspect ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.18.2",
    "fs-extra": "^8.1.0",
    "http-errors": "~1.6.2",
    "mongoose": "^5.8.3",
    "morgan": "~1.9.0",
    "nanoid": "^2.1.8",
    "nodemon": "^1.19.4"
  }
}
